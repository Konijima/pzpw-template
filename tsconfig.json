{
  "compilerOptions": {
    "lib":["ESNext"],
    "target":"ESNext",
    "module":"ESNext",
    "moduleResolution": "node",
    "strict": true,
    "emitDeclarationOnly": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "types": ["lua-types/5.1"]
  },
  "tstl": {
    "noHeader": true,
    "noImplicitSelf": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}